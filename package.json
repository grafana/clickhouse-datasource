{
  "name": "clickhouse-datasource",
  "version": "2.2.0",
  "description": "Clickhouse Datasource",
  "scripts": {
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch",
    "e2e": "grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "generate-release-notes": "PREV_TAG=$(git tag | tail -n 1) && gh api --method POST /repos/grafana/clickhouse-datasource/releases/generate-notes -f tag_name=v${npm_package_version} -f target_commitish=main -f previous_tag_name=${PREV_TAG} | jq -r .body"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "8.5.11",
    "@grafana/e2e": "8.5.11",
    "@grafana/e2e-selectors": "8.5.11",
    "@grafana/runtime": "8.5.11",
    "@grafana/toolkit": "8.5.11",
    "@grafana/ui": "8.5.11",
    "@types/dedent": "^0.7.0",
    "@types/lodash": "latest",
    "cspell": "^6.14.3",
    "dedent": "^0.7.0"
  },
  "resolutions": {
    "rxjs": "^7.5.6"
  },
  "dependencies": {
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "emotion": "^11.0.0",
    "js-sql-parser": "^1.4.1",
    "pgsql-ast-parser": "^11.0.0",
    "tslib": "^2.3.1"
  }
}
